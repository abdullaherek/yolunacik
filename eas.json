{
    "cli": {
      "version": ">= 16.28.0",
      "appVersionSource": "remote"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal"
      },
      "test": {
        "distribution": "store",
        "environment": "development",
        "android": {
          "buildType": "app-bundle"
        },
        "env": {
          "EXPO_PUBLIC_APP_VARIANT": "development",
          "EXPO_PUBLIC_BASE_URL": "https://testapi.yolunacik.com/"
        }
      },
      "production": {
        "distribution": "store",
        "environment": "production",
        "autoIncrement": true,
        "env": {
          "EXPO_PUBLIC_APP_VARIANT": "production",
          "EXPO_PUBLIC_BASE_URL": "https://api.yolunacik.com/"
        }
      }
    },
    "submit": {
      "production": {},
      "test": {}
    }
  }
  